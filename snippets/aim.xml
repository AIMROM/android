  <remote  name="github"
           fetch="https://github.com" />
 
  <remote  name="aim"
           fetch="https://github.com/AIMROM"
	   revision="o8.1"
     	   sync-f="true"
     	   sync-c="true"
	   sync-j="8"
	   review="review.aimrom.org" />

  <remote  name="los"
