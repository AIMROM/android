<?xml version="1.0" encoding="UTF-8"?>
<manifest>

  <!-- Manifest -->
  <project path="manifest" name="manifest" remote="aim" />

  <!-- Build Repos -->
  <project path="build/make" name="build" remote="aim">
    <copyfile src="core/root.mk" dest="Makefile" />
    <linkfile src="CleanSpec.mk" dest="build/CleanSpec.mk" />
    <linkfile src="buildspec.mk.default" dest="build/buildspec.mk.default" />
    <linkfile src="core" dest="build/core" />
    <linkfile src="envsetup.sh" dest="build/envsetup.sh" />
    <linkfile src="target" dest="build/target" />
    <linkfile src="tools" dest="build/tools" />
  </project>
  <project path="build/soong" name="build_soong" remote="aim" >
    <linkfile src="root.bp" dest="Android.bp" />
    <linkfile src="bootstrap.bash" dest="bootstrap.bash" />
  </project>

  <!-- general repos -->
  <project path="bionic" name="android_bionic" remote="aim" />
  <project path="device/lineage/sepolicy" name="device_lineage_sepolicy" remote="aim" />
  <project path="device/qcom/sepolicy" name="device_qcom_sepolicy" remote="aim" />
  <project path="device/qcom/sepolicy-legacy-um" name="device_qcom_sepolicy-legacy-um" remote="aim" />

  <!-- Framework repos -->
  <project path="frameworks/av" name="android_frameworks_av" remote="aim" />
  <project path="frameworks/base" name="android_frameworks_base" remote="aim" />
  <project path="frameworks/native" name="android_frameworks_native" remote="aim" />
  <project path="frameworks/opt/net/ims" name="android_frameworks_opt_net_ims" remote="aim" />
  <project path="frameworks/opt/net/wifi" name="android_frameworks_opt_net_wifi" remote="aim" />
  <project path="frameworks/opt/telephony" name="android_frameworks_opt_telephony" remote="aim" />

  <!-- Hardware repos -->
  <project path="hardware/interfaces" name="android_hardware_interfaces" remote="aim" />
  <project path="hardware/lineage/interfaces" name="android_hardware_lineage_interfaces" remote="aim" />

  <!-- Lineage SDK -->
  <project path="lineage-sdk" name="android_lineage-sdk" remote="aim" />

  <!-- System repos -->
  <project path="system/core" name="system_core" remote="aim"/>
  <project path="system/sepolicy" name="system_sepolicy" remote="aim" />
  <project path="system/vold" name="system_vold" remote="aim" />

  <!-- Packages repos -->
  <project path="packages/apps/Settings" name="Aim_packages_apps_Settings" remote="aim" />
  <project path="packages/apps/ThemePicker" name="packages_apps_ThemePicker" groups="pdk-fs" remote="aim"/>
  <project path="packages/apps/freedomhub" name="packages_apps_freedomhub" remote="aim" />
  <!--project path="packages/apps/Updates" name="packages_apps_Updates" remote="aim" /-->
  <project path="packages/apps/LineageParts" name="packages_apps_LineageParts" remote="aim" />
  <project path="packages/providers/MediaProvider" name="packages_providers_MediaProvider" remote="aim" />
  <project path="packages/apps/SetupWizard" name="packages_apps_SetupWizard" remote="aim" />
  <project path="packages/services/Telecomm" name="packages_services_Telecomm" remote="aim" />
  <project path="packages/services/Telephony" name="packages_services_Telephony" remote="aim" />

  <!-- Vendor -->
  <project path="vendor/aim" name="vendor_aim" remote="aim" />

</manifest>
